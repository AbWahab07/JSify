              data-s-module="LazyImage" 
              alt="{{ section.settings.Image_7_Alt }}" 
            />
          </div>
        </div>
      </div>
    </div>
    <div class="col s12 pull-l6 l6">
      <div class="row">
        <div class="col s12">
          <div class="frame frame-50">
            <img 
              src="{{ section.settings.Image_8 | img_url: '100x' }}"
