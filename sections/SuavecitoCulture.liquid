              data-s-module="LazyImage" 
              alt="{{ section.settings.Image_4_Alt }}" 
            />
          </div>
        </div>
        <div class="col s6">
          <div class="frame frame-100">
            <img
              src="{{ section.settings.Image_5 | img_url: '100x' }}"
              data-src="{{ section.settings.Image_5 | img_url: '600x' }}"
              data-s-module="LazyImage" 
              alt="{{ section.settings.Image_5_Alt }}" 
            />
          </div>
        </div>
      </div>

    </div>
  </div>

  <div class="row">
    <div class="col s12 push-l6 l6">
      <div class="row">
        <div class="col s6">
          <div class="frame frame-100">
            <img
              src="{{ section.settings.Image_6 | img_url: '100x' }}"
