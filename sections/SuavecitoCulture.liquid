<div class="suavecito-culture">

  <div class="row">
    <div class="col s12 push-l6 l6">
      <div class="row">
        <div class="col s6">
          <div class="frame frame-100">
            <img
              src="{{ section.settings.Image_1 | img_url: '100x' }}"
              data-src="{{ section.settings.Image_1 | img_url: '600x' }}"
              data-s-module="LazyImage" 
              alt="{{ section.settings.Image_1_Alt }}" 
            />
          </div>
        </div>
        <div class="col s6">
          <div class="frame frame-100">
            <img
              src="{{ section.settings.Image_2 | img_url: '100x' }}"
              data-src="{{ section.settings.Image_2 | img_url: '600x' }}"
              data-s-module="LazyImage" 
              alt="{{ section.settings.Image_2_Alt }}" 
            />
          </div>
        </div>
      </div>
    </div>
    <div class="col s12 pull-l6 l6">
      <div class="row">
        <div class="col s12">
          <div class="frame frame-50">
            <img 
              src="{{ section.settings.Image_3 | img_url: '100x' }}"
              data-src="{{ section.settings.Image_3 | img_url: '600x' }}"
              data-s-module="LazyImage" 
              alt="{{ section.settings.Image_3_Alt }}" 
            />
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col s12 push-l6 l6">
      <div class="content">
        <div class="content-center">
          <h3 class="center-align">{{ section.settings.Title }}</h3>
          <p>{{ section.settings.Description }}</p>
        </div>
      </div>
    </div>
    <div class="col s12 pull-l6 l6">

      <div class="row">
        <div class="col s6">
          <div class="frame frame-100">
            <img
              src="{{ section.settings.Image_4 | img_url: '100x' }}"
